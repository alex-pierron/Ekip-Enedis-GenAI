    "sentiment_counts = df[\"sentiment\"].value_counts(normalize=False)\n",
    "sentiment_counts_nuanced = df[\"sentiment\"][df[\"nuance\"] == True].value_counts(normalize=False)\n",
    "sentiment_counts_nuanced = sentiment_counts_nuanced.reindex(sentiment_counts.index, fill_value=0)\n",
    "factuel_counts = df[\"factuel\"].value_counts(normalize=True)\n",
    "\n",
    "sentiment_counts_percentage = sentiment_counts / sentiment_counts.sum()\n",
    "sentiment_counts_nuanced_percentage = sentiment_counts_nuanced / sentiment_counts.sum()  \n",
    "\n",
    "fig, axes = plt.subplots(1, 2, figsize=(15, 6))\n",
    "\n",
    "sns.barplot(x=sentiment_counts_percentage.index, y=sentiment_counts_percentage.values, palette=\"deep\", ax=axes[0], label=\"All Sentiments\")\n",
    "sns.barplot(x=sentiment_counts_percentage.index, y=sentiment_counts_nuanced_percentage.values, palette=\"pastel\", ax=axes[0], label=\"Nuanced\")\n",
    "\n",
    "axes[0].set_xlabel(\"Sentiment\")\n",
    "axes[0].set_ylabel(\"Proportion (%)\")\n",
    "axes[0].set_title(\"Distribution des sentiments en pourcentage\")\n",
    "axes[0].set_ylim(0, 1)\n",
    "axes[0].legend()\n",
    "\n",
    "\n",
    "for i, v in enumerate(sentiment_counts_percentage.values):\n",
    "    axes[0].text(i, v + 0.01, f\"{v:.0%}\", ha='center', fontsize=10)\n",
    "for i, v in enumerate(sentiment_counts_nuanced_percentage.values):\n",
    "    axes[0].text(i, v + 0.01, f\"{v:.0%}\", ha='center', fontsize=10)\n",
    "\n",
    "sns.barplot(x=factuel_counts.index.astype(str), y=factuel_counts.values, palette=\"deep\", ax=axes[1])\n",
    "axes[1].set_xlabel(\"Factuel\")\n",
    "axes[1].set_ylabel(\"Proportion\")\n",
    "axes[1].set_title(\"Distribution normalisée des factuels\")\n",
    "axes[1].set_ylim(0, 1)\n",
    "axes[1].legend()\n",
    "\n",
    "for i, v in enumerate(factuel_counts.values):\n",
    "    axes[1].text(i, v + 0.01, f\"{v:.0%}\", ha='center', fontsize=10)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
